   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="stpaverifier.controller.model.STPAVerifierServiceFactory">
         <service
               serviceClass="stpaverifier.controller.IVerifierController"></service>
      </serviceFactory>
      <sourceProvider
            provider="stpaverifier.util.VerificationStateProvider">
         <variable
               name="verifier.run.state"
               priorityLevel="activeContexts">
         </variable>
         <variable
               name="verifier.checker.type"
               priorityLevel="activeContexts">
         </variable>
      </sourceProvider>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="stpaverifier.util.commands.VerificationResumeHandler"
            commandId="stpaVerifier.command.resume">
         <enabledWhen>
            <with
                  variable="verifier.run.state">
               <equals
                     value="paused">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="stpaverifier.util.commands.VerificationCancelHandler"
            commandId="stpaVerifier.command.cancel">
         <enabledWhen>
            <with
                  variable="verifier.run.state">
               <or>
                  <equals
                        value="running">
                  </equals>
                  <equals
                        value="paused">
                  </equals>
               </or>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="stpaverifier.util.commands.VerificationPauseHandler"
            commandId="stpaVerifier.command.pause">
         <enabledWhen>
            <with
                  variable="verifier.run.state">
               <equals
                     value="running">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="stpaverifier.util.commands.AddCTLPropertyHandler"
            commandId="stpaVerifier.command.addCTL">
         <enabledWhen>
            <with
                  variable="verifier.checker.type">
               <equals
                     value="NuSMV">
               </equals>
            </with>
         </enabledWhen>
      </handler>
   </extension>

</plugin>
