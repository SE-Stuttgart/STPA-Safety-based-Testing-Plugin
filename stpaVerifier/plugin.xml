      <perspective
            class="stpaverifier.ModelCheckerPerspective"
            icon="icons/STPA_logoC32.png"
            id="stpaVerifier.perspective"
            name="STPA Verifier">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="stpaverifier.ui.editors.promela.PromelaEditor"
            default="false"
            extensions="pml"
            id="stpaVerifier.editor.text.promela"
            name="Promela">
      </editor>
      <editor
