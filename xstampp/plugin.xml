               </with>
            </visibleWhen>
         </command>
         <command
               commandId="astpa.commands.rename"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <instanceof
                        value="xstampp.ui.navigation.ProjectSelector">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="astpa.save"
               style="push">
            <visibleWhen
                  checkEnabled="false">
