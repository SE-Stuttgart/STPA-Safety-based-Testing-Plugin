            <command
                  commandId="org.eclipse.ui.newWizard"
                  style="push">
            </command>
            <command
                  commandId="astpa.save"
                  style="push">
            </command>
            <command
                  commandId="xstampp.saveall"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.export"
                  style="push">
            </command>
         </toolbar>
         <separator
               name="xstampp.toolbar"
               visible="false">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:astpa.explorer">
         <command
               commandId="astpa.command.openStep"
               id="xstampp.explorer.menu.open"
               label="Open"
               style="toggle">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
