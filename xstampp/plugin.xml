<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/xstampp/logo_small.png">
         <run
               class="xstampp.Application">
         </run>
      </application>
   </extension>
   <extension
         id="xstampp_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="xstampp.application"
            name="STAMP Platform">
         <property
               name="appName"
               value="STAMP Platform">
         </property>
         <property
               name="startupProgressRect"
               value="2,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
        </property>
  <property
        name="windowImages"
        value="icons/xstampp/logox16.png,icons/xstampp/logox32.png,icons/xstampp/logox48.png,icons/xstampp/logox64.png,icons/xstampp/logox128.png">
  </property>
  <property
        name="aboutImage"
        value="icons/xstampp/logox128.png">
  </property>
  <property
        name="aboutText"
        value="XSTAMPP - An eXtensible STAMP Platform for Safety Engineering&#x0A;&#x0A;Â©2014 - 2016&#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/xstampp.html&#x0A;&#x0A;Lukas Balzer, Asim Abdulkhaleq, Stefan Wagner&#x0A;Institute of Software Technology, Software Engineering Group&#x0A;University of Stuttgart, Germany&#x0A;&#x0A;X-STPA &#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/xstpa.html&#x0A;&#x0A;A-CAST&#x0A;http://www.iste.uni-stuttgart.de/se/werkzeuge/a-cast.html">
  </property>
      	</product>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="xstampp.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="xstampp.Perspective"
            fixed="true"
            id="astpa.welcome.perspective"
            name="Welcome perspective">
      </perspective>
      <perspective
            class="xstampp.DefaultPerspective"
            fixed="false"
            icon="icons/xstampp/logox32.png"
            id="xstampp.defaultPerspective"
            name="XSTAMPP">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="xstampp.ui.welcome.WelcomeView"
            id="astpa.Welcome"
            name="Welcome"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="xstampp.ui.navigation.ProjectExplorer"
            fastViewWidthRatio="0.2f"
            id="astpa.explorer"
            name="Project Explorer"
            restorable="true">
      </view>
   </extension>
   <extension
         name="window"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  label="Create New Project"
                  style="push"
                  tooltip="Create a new STPA Project">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
            <command
                  commandId="xstampp.command.load"
                  label="Load Project..."
                  style="push"
                  tooltip="Imports the project file">
            </command>
            <separator
                  name="Separator 1"
                  visible="true">
            </separator>
            <command
                  commandId="astpa.save"
                  style="push">
            </command>
            <command
                  commandId="xstampp.saveall"
                  style="push">
            </command>
            <command
                  commandId="astpa.save"
                  label="Save As..."
                  style="push">
               <parameter
                     name="xstampp.commands.params.doSaveAs"
                     value="TRUE">
               </parameter>
            </command>
            <command
                  commandId="org.eclipse.ui.file.import"
                  label="Import..."
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.export"
                  label="Export..."
                  style="push">
            </command>
            <separator
                  name="Separator 2"
                  visible="true">
            </separator>
            <command
                  commandId="astpa.switchworkspace"
                  id="xstampp.ui.menu.command.switchWS"
                  label="Switch Workspace"
                  style="pulldown"
                  tooltip="Restarts the Platform with the choose workspace dialog">
            </command>
            <command
                  commandId="xstampp.command.openPath"
                  label="Open Workspace"
                  style="push">
               <parameter
                     name="xstampp.commandParameter.openFolder"
                     value="instance">
               </parameter>
            </command>
            <command
                  commandId="astpa.exit"
                  label="Exit Application"
                  style="push"
                  tooltip="Exits the application">
            </command>
         </menu>
         <menu
               id="editMenu"
               label="Edit">
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  helpContextId="Opens the preference page"
                  label="Preferences"
                  style="push">
            </command>
         </menu>
         <menu
               label="Window">
            <command
                  commandId="org.eclipse.ui.window.switchToEditor"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.views.showView"
                  style="pulldown">
            </command>
            <separator
                  name="xstampp.separator1">
            </separator>
            <menu
                  label="Navigation">
               <command
                     commandId="org.eclipse.ui.window.nextView"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.nextEditor"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.switchToEditor"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.previousView"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.ui.window.previousEditor"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.ui.window.resetPerspective"
                  style="push">
            </command>
         </menu>
         <menu
               id="helpMenu"
               label="Help">
            <command
                  commandId="Welcome"
                  style="push"
                  tooltip="Shows the welcome page">
            </command>
            <command
            
            commandId="org.eclipse.ui.help.aboutAction" 
			icon="icons/xstampp/logox16.png" 
			id="x.ex.about" 
			style="push" 
			tooltip="Shows the about dialog">
            </command>
            <command
                  commandId="astpa.update"
                  label="Update"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.help.helpContents"
                  style="push">
            </command>
            <command
                  commandId="xstampp.command.openPath"
                  label="Open Log Path"
                  style="push">
