               style="push">
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/collapseall2.gif"
               label="collapse all"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="False">
            </parameter>
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/expandall.gif"
               label="expand all"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="True">
            </parameter>
         </command>
         <command
               commandId="xstampp.command.openPath"
               label="Open Output"
               style="push">
            <parameter
                  name="xstampp.commandParameter.openFolder"
                  value="output">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:astpa.explorer">
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/collapseall2.gif"
               label="collapse all"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="FALSE">
            </parameter>
         </command>
         <command
               commandId="astpa.commands.expandAll"
               icon="icons/eclipse/expandall.gif"
               label="expand all"
               style="push">
            <parameter
                  name="xstampp.expand"
                  value="TRUE">
            </parameter>
         </command>
         <command
               commandId="org.eclipse.ui.window.preferences"
               style="push">
            <parameter
                  name="preferencePageId"
                  value="astpa.preferencePageColors">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?before=additions">
         <toolbar
               id="xstampp.toolbar.contribution.text">
            <separator
                  name="xstampp.separator1">
            </separator>
            <control
                  class="xstampp.ui.workbench.contributions.TextToolbarContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.editors.interfaces.ITextEditor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
         </toolbar>
         <toolbar
               id="xstampp.toolbar.contribution.zoombar">
            <control
                  class="xstampp.ui.workbench.contributions.SeperatorContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.workbench.contributions.IZoomContributor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
            <control
                  class="xstampp.ui.workbench.contributions.EditorContribution">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeEditor">
                     <instanceof
                           value="xstampp.ui.workbench.contributions.IZoomContributor">
                     </instanceof>
                  </with>
               </visibleWhen>
            </control>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="xstampp.run.toolbar">
            <command
                  commandId="xstampp.command.run"
                  icon="icons/buttons/export/run.png"
                  id="xstampp.toolbar.run.menu"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:xstampp.ui.menu.command.switchWS">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <sourceProvider
            provider="xstampp.ui.menu.file.commands.CommandState">
         <variable
               name="xstampp.ui.menu.file.commands.savestate"
               priorityLevel="workbench">
         </variable>
         <variable
               name="xstampp.ui.menu.file.commands.textstate"
               priorityLevel="workbench">
         </variable>
         <variable
               name="xstampp.ui.menu.file.commands.saveAllState"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="xstampp.ui.menu.file.commands.Save"
            commandId="astpa.save">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.savestate">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.ui.menu.file.commands.SaveAs"
            commandId="astpa.saveas">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.savestate">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.ui.menu.file.commands.SaveAll"
            commandId="xstampp.saveall">
         <enabledWhen>
            <with
                  variable="xstampp.ui.menu.file.commands.saveAllState">
               <equals
                     value="ENABLED">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="xstampp.util.RunExportCommand"
            commandId="xstampp.command.run">
         <enabledWhen>
            <with
                  variable="selection">
               <instanceof
                     value="xstampp.ui.navigation.IProjectSelection">
               </instanceof>
            </with>
         </enabledWhen>
      </handler>ler>
   </extension>
